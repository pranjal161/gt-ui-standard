{
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
      "include": ["src"],
      "includePattern": "\\.(js|jsx|ts|tsx)$",
      "excludePattern": "stories"
    },
    "plugins": [
        "plugins/markdown", 
        "better-docs/component",
        "node_modules/better-docs/typescript"
    ],
    "templates": {
      "better-docs": {
        "name": "My React components"
      }
    },
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md"
    }
  }